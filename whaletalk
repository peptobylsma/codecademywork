let input = 'turpentine and turtles'; // user input to be converted to whale talk
const vowels = ['a','e','i','o','u']; // whales only speak in vowels, this array will be compared to letters in input variable
let resultArray = []; // empty array to store matching elements

for (let i = 0; i < input.length; i++){
 for (let j = 0; j < vowels.length; j++){
   if (input[i] === vowels[j]){
    resultArray.push(input[i]);
   if (input[i] === 'e'){
    resultArray.push(input[i]);
   if (input[i] === 'u'){
     resultArray.push(input[i]);
        }
      }
    }
  }
}

console.log(resultArray.join('').toUpperCase());
